function solicitarNombreCompleto () {
var nombre=prompt ("ingresa tu nombre");
var apellidom=promt ("ingresa apellido materno");
var apellidop=promt ("ingresa apellido paterno");
resultado =nombre +apellidop +apellidom;
console.log ( "el nombre completo es" + resultado );
}

solicitarNombreCompleto (); 
 

